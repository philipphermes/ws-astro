---
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width" />
  <link rel="icon" type="image/svg+xml" href="" />
  <title>{title}</title>
</head>
<body class="text-gray-800">
<header class="p-8 border-b border-b-gray-700">

</header>
<slot />
</body>
</html>
